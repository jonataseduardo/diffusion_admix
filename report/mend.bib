issn = {1095-9203},
journal = {Science (New York, N.Y.)},
keywords = {Adult,Behavior,Cross-Cultural Comparison,Cultural Characteristics,Female,Formal,Government,Humans,Male,Permissiveness,Political Systems,Population Density,Social Behavior,Social Conformity,Social Control,Social Values,Young Adult},
