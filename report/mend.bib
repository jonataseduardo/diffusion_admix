isbn = {0198-8859 (Print)$\backslash$r0198-8859 (Linking)},
issn = {01988859},
journal = {Human Immunology},
keywords = {HLA,antigen recognition site,clinical histocompatibility,common allele,genotype ambiguity,null allele,proficiency testing,well-documented allele},
number = {5},
