isbn = {1471-2164},
issn = {1471-2164},
journal = {BMC genomics},
keywords = {4-primer approach,amplicon-based,dual indexing,fluidigm access array,hla typing,human leukocyte antigen,illumina miseq,ngs,sequencing},
pages = {63},
pmid = {24460756},
title = {{Cost-efficient high-throughput HLA typing by MiSeq amplicon sequencing.}},
