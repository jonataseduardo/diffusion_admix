@article{Maples2013,
abstract = {Local-ancestry inference is an important step in the genetic analysis of fully sequenced human genomes. Current methods can only detect continental-level ancestry (i.e., European versus African versus Asian) accurately even when using millions of markers. Here, we present RFMix, a powerful discriminative modeling approach that is faster (∼30×) and more accurate than existing methods. We accomplish this by using a conditional random field parameterized by random forests trained on reference panels. RFMix is capable of learning from the admixed samples themselves to boost performance and autocorrect phasing errors. RFMix shows high sensitivity and specificity in simulated Hispanics/Latinos and African Americans and admixed Europeans, Africans, and Asians. Finally, we demonstrate that African Americans in HapMap contain modest (but nonzero) levels of Native American ancestry (∼0.4{\%}). {\textcopyright} 2013 The American Society of Human Genetics.},
author = {Maples, Brian K. and Gravel, Simon and Kenny, Eimear E. and Bustamante, Carlos D.},
doi = {10.1016/j.ajhg.2013.06.020},
file = {:home/jonatas/MEGA/Artigos/Maples et al. - 2013 - RFMix A discriminative modeling approach for rapid and robust local-ancestry inference.pdf:pdf},
isbn = {1537-6605 (Electronic)$\backslash$r0002-9297 (Linking)},
