issn = {1362-4962},
journal = {Nucleic acids research},
keywords = {Cluster Analysis,Internet,Mass Spectrometry,Metabolomics,Nuclear Magnetic Resonance, Biomolecular,Software,User-Computer Interface},
month = {jul},
number = {Web Server issue},
pages = {W652--60},
pmid = {19429898},
title = {{MetaboAnalyst: a web server for metabolomic data analysis and interpretation.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2703878{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {37},
year = {2009}
}
