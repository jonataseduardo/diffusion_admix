journal = {Journal of personality and social psychology},
keywords = {Adult,Attitude,Female,Humans,Intuition,Judgment,Male,Morals,Pilot Projects,Questionnaires,Reproducibility of Results,Social Values},
month = {aug},
number = {2},
pages = {366--85},
pmid = {21244182},
title = {{Mapping the moral domain.}},
