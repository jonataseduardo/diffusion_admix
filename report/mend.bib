file = {:home/jonatas/MEGA/Artigos/Lorena, De Carvalho, Gama - 2008 - A review on the combination of binary classifiers in multiclass problems.pdf:pdf},
issn = {02692821},
journal = {Artificial Intelligence Review},
keywords = {Machine learning,Multiclass classification,Supervised learning},
number = {1-4},
