issn = {00222828},
journal = {Journal of Molecular and Cellular Cardiology},
keywords = {Bicuspid aortic valve,GATA5,Pathogenesis,Transcription factor},
number = {2},
pages = {277--281},
pmid = {22641149},
publisher = {Elsevier Ltd},
title = {{Rare non-synonymous variations in the transcriptional activation domains of GATA5 in bicuspid aortic valve disease}},
url = {http://dx.doi.org/10.1016/j.yjmcc.2012.05.009},
volume = {53},
year = {2012}
}
