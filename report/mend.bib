keywords = {Adolescent,Adult,Carnitine,Carnitine: metabolism,Child,Child, Preschool,Humans,Immunohistochemistry,Isoleucine,Isoleucine: metabolism,Magnetic Resonance Spectroscopy,Male,Muscle, Skeletal,Muscle, Skeletal: metabolism,Muscle, Skeletal: pathology,Muscular Dystrophy, Duchenne,Muscular Dystrophy, Duchenne: diagnosis,Muscular Dystrophy, Duchenne: metabolism,Valine,Valine: metabolism},
