keywords = {Dengue,Dengue Virus,Dengue Virus: classification,Dengue Virus: genetics,Dengue Virus: immunology,Dengue: epidemiology,Dengue: immunology,Endemic Diseases,Evolution, Molecular,Humans,Immunity,Immunity: physiology,Molecular Epidemiology,Periodicity,Phylogeny,Serotyping,Stochastic Processes,Thailand,Thailand: epidemiology,Time Factors},
month = {sep},
number = {38},
pages = {14234--9},
pmid = {16966609},
title = {{Cross-protective immunity can account for the alternating epidemic pattern of dengue virus serotypes circulating in Bangkok.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1599940{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {103},
year = {2006}
}
