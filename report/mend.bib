journal = {Journal of computer assisted tomography},
keywords = {Biological Markers,Biological Markers: analysis,Child,Creatinine,Creatinine: analysis,Duchenne,Duchenne: diagnosis,Duchenne: metabolism,Female,Humans,Magnetic Resonance Spectroscopy,Magnetic Resonance Spectroscopy: methods,Male,Muscle,Muscular Dystrophy,Protons,Protons: diagnostic use,Skeletal,Skeletal: metabolism,Trimethyl Ammonium Compounds,Trimethyl Ammonium Compounds: analysis},
