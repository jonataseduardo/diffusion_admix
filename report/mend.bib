@article{Poncela-Casasnovas2016,
abstract = {Socially relevant situations that involve strategic interactions are widespread among animals and humans alike. To study these situations, theoretical and experimental research has adopted a game theoretical perspective, generating valuable insights about human behavior. However, most of the results reported so far have been obtained from a population perspective and considered one specific conflicting situation at a time. This makes it difficult to extract conclusions about the consistency of individuals' behavior when facing different situations and to define a comprehensive classification of the strategies underlying the observed behaviors. We present the results of a lab-in-the-field experiment in which subjects face four different dyadic games, with the aim of establishing general behavioral rules dictating individuals' actions. By analyzing our data with an unsupervised clustering algorithm, we find that all the subjects conform, with a large degree of consistency, to a limited number of behavioral phenotypes (envious, optimist, pessimist, and trustful), with only a small fraction of undefined subjects. We also discuss the possible connections to existing interpretations based on a priori theoretical approaches. Our findings provide a relevant contribution to the experimental and theoretical efforts toward the identification of basic behavioral phenotypes in a wider set of contexts without aprioristic assumptions regarding the rules or strategies behind actions. From this perspective, our work contributes to a fact-based approach to the study of human behavior in strategic situations, which could be applied to simulating societies, policy-making scenario building, and even a variety of business applications.},
archivePrefix = {arXiv},
arxivId = {1608.02015},
author = {Poncela-Casasnovas, Julia and Guti{\'{e}}rrez-Roig, Mario and Gracia-L{\'{a}}zaro, Carlos and Vicens, Julian and G{\'{o}}mez-Garde{\~{n}}es, Jes{\'{u}}s and Perell{\'{o}}, Josep and Moreno, Yamir and Duch, Jordi and S{\'{a}}nchez, Angel},
doi = {10.1126/sciadv.1600451},
eprint = {1608.02015},
file = {:home/jonatas/MEGA/Artigos/Poncela-Casasnovas et al. - 2016 - Humans display a reduced set of consistent behavioral phenotypes in dyadic games.pdf:pdf},
issn = {2375-2548},
journal = {Science advances},
number = {8},
pages = {e1600451},
pmid = {27532047},
title = {{Humans display a reduced set of consistent behavioral phenotypes in dyadic games.}},
url = {http://arxiv.org/abs/1608.02015},
volume = {2},
year = {2016}
}
@article{Lin2012,
abstract = {Learning to make choices that yield rewarding outcomes requires the computation of three distinct signals: stimulus values that are used to guide choices at the time of decision making, experienced utility signals that are used to evaluate the outcomes of those decisions and prediction errors that are used to update the values assigned to stimuli during reward learning. Here we investigated whether monetary and social rewards involve overlapping neural substrates during these computations. Subjects engaged in two probabilistic reward learning tasks that were identical except that rewards were either social (pictures of smiling or angry people) or monetary (gaining or losing money). We found substantial overlap between the two types of rewards for all components of the learning process: a common area of ventromedial prefrontal cortex (vmPFC) correlated with stimulus value at the time of choice and another common area of vmPFC correlated with reward magnitude and common areas in the striatum correlated with prediction errors. Taken together, the findings support the hypothesis that shared anatomical substrates are involved in the computation of both monetary and social rewards.},
author = {Lin, Alice and Adolphs, Ralph and Rangel, Antonio},
doi = {10.1093/scan/nsr006},
file = {:home/jonatas/MEGA/Artigos/Lin, Adolphs, Rangel - 2012 - Social and monetary reward learning engage overlapping neural substrates.pdf:pdf},
issn = {1749-5024},
journal = {Social cognitive and affective neuroscience},
keywords = {Adolescent,Adult,Brain,Brain Mapping,Brain: blood supply,Brain: physiology,Choice Behavior,Female,Humans,Image Processing, Computer-Assisted,Learning,Learning: physiology,Magnetic Resonance Imaging,Oxygen,Oxygen: blood,Photic Stimulation,Reward,Social Behavior,Statistics as Topic,Time Factors,Young Adult},
month = {mar},
number = {3},
pages = {274--81},
pmid = {21427193},
title = {{Social and monetary reward learning engage overlapping neural substrates.}},
