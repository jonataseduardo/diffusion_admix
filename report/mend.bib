keywords = {Breast Neoplasms,Breast Neoplasms: genetics,Computer Simulation,Female,Gene Expression Profiling,Gene Expression Profiling: methods,Gene Expression Regulation,Genetic,Humans,Models,Neoplasm Proteins,Neoplasm Proteins: genetics,Neoplasms,Neoplasms: genetics,Neoplasms: pathology,Neoplastic,Predictive Value of Tests,Prognosis,Reproducibility of Results,Treatment Outcome},
month = {apr},
number = {15},
pages = {5923--8},
pmid = {16585533},
title = {{Thousands of samples are needed to generate a robust gene list for predicting outcome in cancer.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1458674{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {103},
