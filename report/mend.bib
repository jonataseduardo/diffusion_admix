@article{Behr2016,
abstract = {Motivation: A series of methods in population genetics use multilocus genotype data to assign individuals membership in latent clusters. These methods belong to a broad class of mixed-membership models, such as latent Dirichlet allocation used to analyze text corpora. Inference from mixed-membership models can produce different output matrices when repeatedly applied to the same inputs, and the number of latent clusters is a parameter that is often varied in the analysis pipeline. For these reasons, quantifying, visualizing, and annotating the output from mixed-membership models are bottlenecks for investigators across multiple disciplines from ecology to text data mining.$\backslash$nResults: We introduce pong, a network-graphical approach for analyzing and visualizing membership in latent clusters with a native D3.js interactive visualization. pong leverages efficient algorithms for solving the Assignment Problem to dramatically reduce runtime while increasing accuracy compared to other methods that process output from mixed-membership models. We apply pong to 225,705 unlinked genome-wide single-nucleotide variants from 2,426 unrelated individuals in the 1000 Genomes Project, and identify previously overlooked aspects of global human population structure.We show that pong outpaces current solutions by more than an order of magnitude in runtime while providing a customizable and interactive visualization of population structure that is more accurate than those produced by current tools.$\backslash$nAvailability: pong is freely available and can be installed using the Python package management system pip. pong's source code is available at https://github.com/abehr/pong.$\backslash$nContact: aaron{\_}behr{\{}at{\}}alumni.brown.edu, sramachandran{\{}at{\}}brown.edu$\backslash$nSupplementary Information: Supplementary Text and Figures are available on the journal website.},
author = {Behr, Aaron A. and Liu, Katherine Z. and Liu-Fang, Gracie and Nakka, Priyanka and Ramachandran, Sohini},
doi = {10.1093/bioinformatics/btw327},
file = {:home/jonatas/MEGA/Artigos/Behr et al. - 2016 - Pong Fast Analysis and Visualization of Latent Clusters in Population Genetic Data.pdf:pdf},
issn = {1367-4803, 1460-2059},
journal = {Bioinformatics},
number = {June},
pages = {btw327},
title = {{Pong: Fast Analysis and Visualization of Latent Clusters in Population Genetic Data}},
url = {http://bioinformatics.oxfordjournals.org/content/early/2016/06/07/bioinformatics.btw327{\%}5Cnhttp://bioinformatics.oxfordjournals.org/content/early/2016/06/07/bioinformatics.btw327.full.pdf{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/27283948},
