keywords = {Animals,Kidney,Kidney: drug effects,Liver,Liver: drug effects,Magnetic Resonance Spectroscopy,Magnetic Resonance Spectroscopy: methods,Male,Pattern Recognition, Automated,Rats,Rats, Inbred F344,Rats, Inbred Strains,Toxicology,Toxicology: methods,Toxins, Biological,Toxins, Biological: classification,Toxins, Biological: urine},
month = {may},
number = {5},
pages = {629--42},
pmid = {2034235},
title = {{Application of pattern recognition methods to the analysis and classification of toxicological data derived from proton nuclear magnetic resonance spectroscopy of urine.}},
