@article{Kehdy2015a,
abstract = {While South Americans are underrepresented in human genomic diversity studies, Brazil has been a classical model for population genetics studies on admixture. We present the results of the EPIGEN Brazil Initiative, the most comprehensive up-to-date genomic analysis of any Latin-American population. A population-based genome-wide analysis of 6,487 individuals was performed in the context of worldwide genomic diversity to elucidate how ancestry, kinship, and inbreeding interact in three populations with different histories from the Northeast (African ancestry: 50{\%}), Southeast, and South (both with European ancestry {\textgreater}70{\%}) of Brazil. We showed that ancestry-positive assortative mating permeated Brazilian history. We traced European ancestry in the Southeast/South to a wider European/Middle Eastern region with respect to the Northeast, where ancestry seems restricted to Iberia. By developing an approximate Bayesian computation framework, we infer more recent European immigration to the Southeast/South than to the Northeast. Also, the observed low Native-American ancestry (6–8{\%}) was mostly introduced in different regions of Brazil soon after the European Conquest. We broadened our understanding of the African diaspora, the major destination of which was Brazil, by revealing that Brazilians display two within-Africa ancestry components: one associated with non-Bantu/western Africans (more evident in the Northeast and African Americans) and one associated with Bantu/eastern Africans (more present in the Southeast/South). Furthermore, the whole-genome analysis of 30 individuals (42-fold deep coverage) shows that continental admixture rather than local post-Columbian history is the main and complex determinant of the individual amount of deleterious genotypes.},
author = {Kehdy, Fernanda S. G. and Gouveia, Mateus H. and Machado, Moara and Magalh{\~{a}}es, Wagner C. S. and Horimoto, Andrea R. and Horta, Bernardo L. and Moreira, Rennan G. and Leal, Thiago P. and Scliar, Marilia O. and Soares-Souza, Giordano B. and Rodrigues-Soares, Fernanda and Ara{\'{u}}jo, Gilderlanio S. and Zamudio, Roxana and Anna, Hanaisa P. Sant and Santos, Hadassa C. and Duarte, Nubia E. and Fiaccone, Rosemeire L. and Figueiredo, Camila a. and Silva, Thiago M. and Costa, Gustavo N. O. and Beleza, Sandra and Berg, Douglas E. and Cabrera, Lilia and Debortoli, Guilherme and Duarte, Denise and Ghirotto, Silvia and Gilman, Robert H. and Gon{\c{c}}alves, Vanessa F. and Marrero, Andrea R. and Muniz, Yara C. and Weissensteiner, Hansi and Yeager, Meredith and Rodrigues, Laura C. and Barreto, Mauricio L. and Lima-Costa, M. Fernanda and Pereira, Alexandre C. and Rodrigues, Ma{\'{i}}ra R. and Tarazona-Santos, Eduardo and Consortium, The Brazilian Epigen Project and Alcantara-Neves, Neuza and Ara{\'{u}}jo, Nathalia M. and Carvalho, M{\'{a}}rcio L. B. and Concei{\c{c}}{\~{a}}o, Jackson Santos and Firmo, Jos{\'{e}}lia O. a. and Gigante, Denise P. and Meira, Lindolfo and Muniz-Queiroz, Thais and Oliveira, Guilherme C. and Oliveira, Isabel O. and Peixoto, S{\'{e}}rgio V. and Proietti, Fernando a. and Rodrigues, Domingos C. and Santolalla, Meddly L. and Strina, Agostino and Zolini, Camila},
doi = {10.1073/pnas.1504447112},
file = {:home/jonatas/MEGA/Artigos/Kehdy et al. - 2015 - Origin and dynamics of admixture in Brazilians and its effect on the pattern of deleterious mutations(2).pdf:pdf},
issn = {0027-8424, 1091-6490},
journal = {Proceedings of the National Academy of Sciences},
number = {28},
pages = {8696--8701},
pmid = {26124090},
title = {{Origin and dynamics of admixture in Brazilians and its effect on the pattern of deleterious mutations}},
url = {http://www.pnas.org/content/112/28/8696{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/26124090{\%}5Cnhttp://www.pnas.org/content/112/28/8696.full},
volume = {112},
