issn = {1367-4811},
journal = {Bioinformatics (Oxford, England)},
keywords = {Algorithms,Chromosome Mapping,Chromosome Mapping: methods,High-Throughput Nucleotide Sequencing,High-Throughput Nucleotide Sequencing: methods},
month = {oct},
number = {20},
pages = {2592--9},
pmid = {22923295},
title = {{RazerS 3: faster, fully sensitive read mapping.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22923295},
volume = {28},
year = {2012}
