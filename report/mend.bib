isbn = {1476-4687 (Electronic)$\backslash$n0028-0836 (Linking)},
issn = {1476-4687},
journal = {Nature},
keywords = {Africa,Africa: ethnology,African Continental Ancestry Group,African Continental Ancestry Group: genetics,Alleles,Europe,Europe: ethnology,European Continental Ancestry Group,European Continental Ancestry Group: genetics,Evolution, Molecular,Exome,Exome: genetics,Exons,Exons: genetics,Genetic Variation,Genetic Variation: genetics,Humans,Open Reading Frames,Open Reading Frames: genetics,Polymorphism, Single Nucleotide,Polymorphism, Single Nucleotide: genetics,United States},
number = {7431},
