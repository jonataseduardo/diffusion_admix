keywords = {Animals,Brain,Brain: physiology,Discriminant Analysis,Gene Expression Profiling,Gene Expression Profiling: methods,Genetic Diseases, Inborn,Genetic Diseases, Inborn: metabolism,Heart,Heart: physiology,Linear Models,Magnetic Resonance Spectroscopy,Magnetic Resonance Spectroscopy: diagnostic use,Male,Mice,Mice, Inbred C57BL,Mice, Inbred mdx,Muscles,Muscles: metabolism,Muscles: pathology,Muscular Dystrophy, Animal,Muscular Dystrophy, Animal: metabolism,Phenotype},
month = {jun},
