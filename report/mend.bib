issn = {1476-4687},
journal = {Nature},
keywords = {Autoimmune Diseases,Autoimmune Diseases: genetics,European Continental Ancestry Group,European Continental Ancestry Group: genetics,Exons,Exons: genetics,Gene Frequency,Genetic Predisposition to Disease,Genetic Predisposition to Disease: genetics,Genetic Variation,Genetic Variation: genetics,Genome-Wide Association Study,Great Britain,Humans,Models, Genetic,Mutation,Mutation: genetics,Open Reading Frames,Open Reading Frames: genetics,Phenotype,Sample Size},
