keywords = {Adolescent,Adult,Aggression,Aggression: psychology,Defense Mechanisms,England,European Continental Ancestry Group,European Continental Ancestry Group: psychology,Existentialism,Female,France,Group Processes,Humans,Interpersonal Relations,Male,Scotland,Social Behavior,Social Identification,Social Values,Students,Young Adult},
month = {jan},
